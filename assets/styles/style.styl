	display block

	div.sideNav
		display block
		animation moveSideNav 0.2s

	div.backContainer
		background-color rgba(0,0,0,0.5)
		animation backAnimation 0.7s
		animation-duration: 0.9s;
		animation-iteration-count: 1;
		animation-direction: alternate;
		animation-timing-function: ease-in-out;

div.hideMenuContainer

	div.sideNav
		animation hideSideNav 0.2s
		transform translateX(-110%)

	div.backContainer
		transition all 0.2s ease
		opacity 0

.sideNavMenuContainer
	width 100%;
	height 100vh
	position fixed
	top 0
	left 0
	z-index 2
	display none
	color: rgba(0,0,0,0.54);

	div.sideNav
		width 20%
		background-color #fff
		float left
		height 100vh
		display none

		div.upSideNav
			padding 18px 12px 12px
			background-color rgb(114, 36, 108)
			background -webkit-linear-gradient(rgb(60, 16, 83), rgb(114, 36, 108))
			background -o-linear-gradient(rgb(60, 16, 83), rgb(114, 36, 108))
			background -moz-linear-gradient(rgb(60, 16, 83), rgb(114, 36, 108))
			background linear-gradient(rgb(60, 16, 83), rgb(114, 36, 108))
			color: rgba(255,255,255,0.7);

			div.pictureSideNavContainer
				height 80px
				width 80px
				border-radius 50%;
				background-position 50%
				background-size cover
			div.nameEmailContainer
				margin-top 25px
				font-size 15px
				div.nameMenu
					font-weight 500
				div.emailMenu
					font-weight 400

		div.downSideNav
			min-height 200px
			padding 0 12px
			ul
				li
					cursor pointer
					height 46px
					line-height 46px
					a
						width 100%
						height 100%
						display block
						i
							display inline-block
							margin-left 0px;
							font-size 20px
							transition color 1s ease

						div.menu-item-sidenav
							display inline-block
							margin-left 10px
							font-size 15px
							font-weight 500
							transition color 1s ease

				li:hover
					a
						i
						div.menu-item-sidenav
							color: rgb(114, 36, 108)

				/*li.lastItemSideNav*/

	div.backContainer
		width 80%
		float right
		height 100vh
		transition background 2s ease

