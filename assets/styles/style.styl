div.menuTitleSearchContainer
	width 100%
	padding 0 2.5% 20px
	font-size 24px
	padding-top 30px
	display none
	background-color rgb(60, 16, 83)
	color #f3f3f3

	i.iconMenu
		width 5%
		display inline-block
		cursor pointer

	div.title
		width 60%
		margin 0
		font-size 24px
		display inline-block

